{
  "name": "web-components",
  "version": "1.0.0",
  "description": "Web Components",
  "main": "index.js",
  "scripts": {
    "start": "run-p start:rollup start:stencil serve",
    "build": "npm run build:rollup && npm run build:stencil",
    "start:rollup": "rollup -c -w",
    "build:rollup": "rollup -c",
    "start:stencil": "stencil build --dev --watch",
    "build:stencil": "stencil build",
    "serve": "serve public",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest src",
    "test:generate-output": "npm run test -- --json --outputFile=.jest-test-results.json",
    "test:watch": "npm run test:generate-output -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-jest": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-notes": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/svelte": "^5.2.6",
    "@storybook/theming": "^5.2.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/svelte": "^1.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-transform-svelte": "^2.1.0",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rollup": "^1.26.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.1.2",
    "serve": "^11.2.0",
    "svelte": "^3.49.0",
    "svelte-loader": "^2.13.6",
    "workbox-build": "4.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js, jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@stencil/core": "^1.7.5",
    "lit-element": "^2.2.1",
    "modern-css-reset": "^1.0.4"
  }
}
